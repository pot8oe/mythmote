<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    android:orientation="vertical"
    android:weightSum="50" >
    
    <TableRow
        android:layout_weight="10"
        android:weightSum="50" >

        <tkj.android.homecontrol.mythmote.ui.AutoRepeatImageButton
            android:id="@+id/ButtonChUp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="0.5dip"
            android:layout_weight="10"
            android:background="@drawable/mythmote_button_dark_square"
            android:src="@drawable/tv_plus" />
    
	     <tkj.android.homecontrol.mythmote.ui.AutoRepeatImageButton
	         android:id="@+id/ButtonInfo"
	         android:layout_width="match_parent"
	         android:layout_height="match_parent"
	         android:layout_margin="0.5dip"
	         android:layout_weight="10"
	         android:background="@drawable/mythmote_button_dark_square"
	         android:src="@drawable/info" />
	
	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatImageButton
	        android:id="@+id/ButtonUp"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_margin="0.5dip"
	        android:layout_weight="10"
	        android:background="@drawable/mythmote_button_dark_square"
	        android:src="@drawable/up" />
	    
	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatImageButton
	        android:id="@+id/ButtonGuide"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_margin="0.5dip"
	        android:layout_weight="10"
	        android:background="@drawable/mythmote_button_dark_square"
	        android:src="@drawable/guide" />

	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatImageButton
	        android:id="@+id/ButtonVolUp"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_margin="0.5dip"
	        android:layout_weight="10"
	        android:background="@drawable/mythmote_button_dark_square"
	        android:src="@drawable/speaker_plus" />
    
    </TableRow>
    
    
    <TableRow android:layout_weight="10"
        android:weightSum="50">

        <tkj.android.homecontrol.mythmote.ui.AutoRepeatImageButton
            android:id="@+id/ButtonChReturn"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="0.5dip"
            android:layout_weight="10"
            android:background="@drawable/mythmote_button_dark_square"
            android:src="@drawable/tv_back" />
    
	     <tkj.android.homecontrol.mythmote.ui.AutoRepeatImageButton
	         android:id="@+id/ButtonLeft"
	         android:layout_width="match_parent"
	         android:layout_height="match_parent"
	         android:layout_margin="0.5dip"
	         android:layout_weight="10"
	         android:background="@drawable/mythmote_button_dark_square"
	         android:src="@drawable/left" />
	
	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatImageButton
	        android:id="@+id/ButtonSelect"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_margin="0.5dip"
	        android:layout_weight="10"
	        android:background="@drawable/mythmote_button_dark_square"
	        android:src="@drawable/square" />
	    
	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatImageButton
	        android:id="@+id/ButtonRight"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_margin="0.5dip"
	        android:layout_weight="10"
	        android:background="@drawable/mythmote_button_dark_square"
	        android:src="@drawable/right" />

	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatImageButton
	        android:id="@+id/ButtonMute"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_margin="0.5dip"
	        android:layout_weight="10"
	        android:background="@drawable/mythmote_button_dark_square"
	        android:src="@drawable/speaker_off" />
    
    </TableRow>
    
    <TableRow android:layout_weight="10"
        android:weightSum="50">

        <tkj.android.homecontrol.mythmote.ui.AutoRepeatImageButton
            android:id="@+id/ButtonChDown"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="0.5dip"
            android:layout_weight="10"
            android:background="@drawable/mythmote_button_dark_square"
            android:src="@drawable/tv_minus" />
    
	     <tkj.android.homecontrol.mythmote.ui.AutoRepeatImageButton
	         android:id="@+id/ButtonEsc"
	         android:layout_width="match_parent"
	         android:layout_height="match_parent"
	         android:layout_margin="0.5dip"
	         android:layout_weight="10"
	         android:background="@drawable/mythmote_button_dark_square"
	         android:src="@drawable/cancel" />
	
	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatImageButton
	        android:id="@+id/ButtonDown"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_margin="0.5dip"
	        android:layout_weight="10"
	        android:background="@drawable/mythmote_button_dark_square"
	        android:src="@drawable/down" />
	    
	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatImageButton
	        android:id="@+id/ButtonMenu"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_margin="0.5dip"
	        android:layout_weight="10"
	        android:background="@drawable/mythmote_button_dark_square"
	        android:src="@drawable/menu" />

	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatImageButton
	        android:id="@+id/ButtonVolDown"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_margin="0.5dip"
	        android:layout_weight="10"
	        android:background="@drawable/mythmote_button_dark_square"
	        android:src="@drawable/speaker_minus" />
    
    </TableRow>
    
    <TableRow android:layout_weight="10"
        android:weightSum="40">
    
	     <tkj.android.homecontrol.mythmote.ui.AutoRepeatImageButton
	         android:id="@+id/ButtonRecord"
	         android:layout_width="match_parent"
	         android:layout_height="match_parent"
	         android:layout_margin="0.5dip"
	         android:layout_weight="10"
	         android:background="@drawable/mythmote_button_dark_square"
	         android:src="@drawable/ic_media_tango_record" />
	
	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatImageButton
	        android:id="@+id/ButtonStop"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_margin="0.5dip"
	        android:layout_weight="10"
	        android:background="@drawable/mythmote_button_dark_square"
	        android:src="@drawable/ic_media_tango_stop" />
	    
	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatImageButton
	        android:id="@+id/ButtonPause"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_margin="0.5dip"
	        android:layout_weight="10"
	        android:background="@drawable/mythmote_button_dark_square"
	        android:src="@drawable/ic_media_tango_pause" />
	    
	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatImageButton
	        android:id="@+id/ButtonPlay"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_margin="0.5dip"
	        android:layout_weight="10"
	        android:background="@drawable/mythmote_button_dark_square"
	        android:src="@drawable/ic_media_tango_play" />
    
    </TableRow>
    
    
    <TableRow android:layout_weight="10"
        android:weightSum="40">
    
	     <tkj.android.homecontrol.mythmote.ui.AutoRepeatImageButton
	         android:id="@+id/ButtonSkipBack"
	         android:layout_width="match_parent"
	         android:layout_height="match_parent"
	         android:layout_margin="0.5dip"
	         android:layout_weight="10"
	         android:background="@drawable/mythmote_button_dark_square"
	         android:src="@drawable/ic_media_tango_previous" />
	
	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatImageButton
	        android:id="@+id/ButtonRew"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_margin="0.5dip"
	        android:layout_weight="10"
	        android:background="@drawable/mythmote_button_dark_square"
	        android:src="@drawable/ic_media_tango_rew" />
	    
	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatImageButton
	        android:id="@+id/ButtonFF"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_margin="0.5dip"
	        android:layout_weight="10"
	        android:background="@drawable/mythmote_button_dark_square"
	        android:src="@drawable/ic_media_tango_ff" />
	    
	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatImageButton
	        android:id="@+id/ButtonSkipForward"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_margin="0.5dip"
	        android:layout_weight="10"
	        android:background="@drawable/mythmote_button_dark_square"
	        android:src="@drawable/ic_media_tango_next" />
    
    </TableRow>
    
    
    
</TableLayout>
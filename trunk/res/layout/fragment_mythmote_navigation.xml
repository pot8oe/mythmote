<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    android:orientation="vertical"
    android:weightSum="70" >
    
    <TableRow android:layout_weight="10"
        android:weightSum="30">
    
	     <tkj.android.homecontrol.mythmote.ui.AutoRepeatButton
	         android:id="@+id/ButtonInfo"
	         android:layout_width="match_parent"
	         android:layout_height="match_parent"
	         android:layout_marginBottom="1dp"
	         android:layout_marginLeft="1dp"
	         android:layout_marginTop="1dp"
	         android:layout_weight="10"
	         android:background="@drawable/mythmote_button"
	         android:src="@drawable/info" />
	
	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatButton
	        android:id="@+id/ButtonUp"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_marginBottom="1dp"
	        android:layout_marginLeft="1dp"
	        android:layout_marginRight="1dp"
	        android:layout_marginTop="1dp"
	        android:layout_weight="10"
	        android:background="@drawable/mythmote_button"
	        android:src="@drawable/up" />
	    
	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatButton
	        android:id="@+id/ButtonGuide"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_marginBottom="1dp"
	        android:layout_marginRight="1dp"
	        android:layout_marginTop="1dp"
	        android:layout_weight="10"
	        android:background="@drawable/mythmote_button"
	        android:src="@drawable/guide" />
    
    </TableRow>
    
    
    <TableRow android:layout_weight="10"
        android:weightSum="30">
    
	     <tkj.android.homecontrol.mythmote.ui.AutoRepeatButton
	         android:id="@+id/ButtonLeft"
	         android:layout_width="match_parent"
	         android:layout_height="match_parent"
	         android:layout_marginBottom="1dp"
	         android:layout_marginLeft="1dp"
	         android:layout_weight="10"
	         android:background="@drawable/mythmote_button"
	         android:src="@drawable/left" />
	
	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatButton
	        android:id="@+id/ButtonSelect"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_marginBottom="1dp"
	        android:layout_marginLeft="1dp"
	        android:layout_marginRight="1dp"
	        android:layout_weight="10"
	        android:background="@drawable/mythmote_button"
	        android:src="@drawable/square" />
	    
	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatButton
	        android:id="@+id/ButtonRight"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_marginBottom="1dp"
	        android:layout_marginRight="1dp"
	        android:layout_weight="10"
	        android:background="@drawable/mythmote_button"
	        android:src="@drawable/right" />
    
    </TableRow>
    
    <TableRow android:layout_weight="10"
        android:weightSum="30">
    
	     <tkj.android.homecontrol.mythmote.ui.AutoRepeatButton
	         android:id="@+id/ButtonEsc"
	         android:layout_width="match_parent"
	         android:layout_height="match_parent"
	         android:layout_marginBottom="1dp"
	         android:layout_marginLeft="1dp"
	         android:layout_weight="10"
	         android:background="@drawable/mythmote_button"
	         android:src="@drawable/cancel" />
	
	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatButton
	        android:id="@+id/ButtonDown"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_marginBottom="1dp"
	        android:layout_marginLeft="1dp"
	        android:layout_marginRight="1dp"
	        android:layout_weight="10"
	        android:background="@drawable/mythmote_button"
	        android:src="@drawable/down" />
	    
	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatButton
	        android:id="@+id/ButtonMenu"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_marginBottom="1dp"
	        android:layout_marginRight="1dp"
	        android:layout_weight="10"
	        android:background="@drawable/mythmote_button"
	        android:src="@drawable/menu" />
    
    </TableRow>
    
    <TableRow android:layout_weight="10"
        android:weightSum="30">
        
		<tkj.android.homecontrol.mythmote.ui.AutoRepeatButton
		    android:id="@+id/ButtonChUp"
		    android:layout_width="match_parent"
		    android:layout_height="match_parent"
		    android:layout_marginBottom="1dp"
		    android:layout_marginLeft="1dp"
		    android:layout_weight="10"
		    android:background="@drawable/mythmote_button"
		    android:src="@drawable/tv_plus" />
			
		<tkj.android.homecontrol.mythmote.ui.AutoRepeatButton
		    android:id="@+id/ButtonChReturn"
		    android:layout_width="match_parent"
		    android:layout_height="match_parent"
		    android:layout_marginBottom="1dp"
		    android:layout_marginLeft="1dp"
		    android:layout_marginRight="1dp"
		    android:layout_weight="10"
		    android:background="@drawable/mythmote_button"
		    android:src="@drawable/tv_back" />
			   
		<tkj.android.homecontrol.mythmote.ui.AutoRepeatButton
		    android:id="@+id/ButtonChDown"
		    android:layout_width="match_parent"
		    android:layout_height="match_parent"
		    android:layout_marginBottom="1dp"
		    android:layout_marginRight="1dp"
		    android:layout_weight="10"
		    android:background="@drawable/mythmote_button"
		    android:src="@drawable/tv_minus" />
	
    </TableRow>
    
    <TableRow android:layout_weight="10"
        android:weightSum="30">
        	
        <tkj.android.homecontrol.mythmote.ui.AutoRepeatButton
            android:id="@+id/ButtonVolUp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="1dp"
            android:layout_marginLeft="1dp"
            android:layout_weight="10"
            android:background="@drawable/mythmote_button"
            android:src="@drawable/speaker_plus" />
	
	   <tkj.android.homecontrol.mythmote.ui.AutoRepeatButton
	       android:id="@+id/ButtonMute"
	       android:layout_width="match_parent"
	       android:layout_height="match_parent"
	       android:layout_marginBottom="1dp"
	       android:layout_marginLeft="1dp"
	       android:layout_marginRight="1dp"
	       android:layout_weight="10"
	       android:background="@drawable/mythmote_button"
	       android:src="@drawable/speaker_off" />
	   
	   <tkj.android.homecontrol.mythmote.ui.AutoRepeatButton
	       android:id="@+id/ButtonVolDown"
	       android:layout_width="match_parent"
	       android:layout_height="match_parent"
	       android:layout_marginBottom="1dp"
	       android:layout_marginRight="1dp"
	       android:layout_weight="10"
	       android:background="@drawable/mythmote_button"
	       android:src="@drawable/speaker_minus" />
	   
    </TableRow>
    
    <TableRow android:layout_weight="10"
        android:weightSum="100">
    
	     <tkj.android.homecontrol.mythmote.ui.AutoRepeatButton
	         android:id="@+id/ButtonRecord"
	         android:layout_width="match_parent"
	         android:layout_height="match_parent"
	         android:layout_marginBottom="1dp"
	         android:layout_marginLeft="1dp"
	         android:layout_marginRight="1dp"
	         android:layout_weight="25"
	         android:background="@drawable/mythmote_button"
	         android:src="@drawable/rec" />
	
	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatButton
	        android:id="@+id/ButtonStop"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_marginBottom="1dp"
	        android:layout_marginRight="1dp"
	        android:layout_weight="25"
	        android:background="@drawable/mythmote_button"
	        android:src="@drawable/stop" />
	    
	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatButton
	        android:id="@+id/ButtonPause"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_marginBottom="1dp"
	        android:layout_marginRight="1dp"
	        android:layout_weight="25"
	        android:background="@drawable/mythmote_button"
	        android:src="@drawable/pause" />
	    
	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatButton
	        android:id="@+id/ButtonPlay"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_marginBottom="1dp"
	        android:layout_marginRight="1dp"
	        android:layout_weight="25"
	        android:background="@drawable/mythmote_button"
	        android:src="@drawable/play" />
    
    </TableRow>
    
    
    <TableRow android:layout_weight="10"
        android:weightSum="100">
    
	     <tkj.android.homecontrol.mythmote.ui.AutoRepeatButton
	         android:id="@+id/ButtonSkipBack"
	         android:layout_width="match_parent"
	         android:layout_height="match_parent"
	         android:layout_marginBottom="1dp"
	         android:layout_marginLeft="1dp"
	         android:layout_marginRight="1dp"
	         android:layout_weight="25"
	         android:background="@drawable/mythmote_button"
	         android:src="@drawable/skipbk" />
	
	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatButton
	        android:id="@+id/ButtonRew"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_marginBottom="1dp"
	        android:layout_marginRight="1dp"
	        android:layout_weight="25"
	        android:background="@drawable/mythmote_button"
	        android:src="@drawable/rw" />
	    
	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatButton
	        android:id="@+id/ButtonFF"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_marginBottom="1dp"
	        android:layout_marginRight="1dp"
	        android:layout_weight="25"
	        android:background="@drawable/mythmote_button"
	        android:src="@drawable/ff" />
	    
	    <tkj.android.homecontrol.mythmote.ui.AutoRepeatButton
	        android:id="@+id/ButtonSkipForward"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_marginBottom="1dp"
	        android:layout_marginRight="1dp"
	        android:layout_weight="25"
	        android:background="@drawable/mythmote_button"
	        android:src="@drawable/skipfw" />
    
    </TableRow>
    
    
    
</TableLayout>